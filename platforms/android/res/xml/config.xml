<?xml version='1.0' encoding='utf-8'?>
<widget id="com.oshnic.gmsys" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>OSPLGmSys</name>
    <description>
        Oshnic's Godown Management System
    </description>
    <author email="info@osindia.in" href="http://osindia.in">
        Team Oshnic 
    </author>
    <content src="auth.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
